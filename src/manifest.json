{
  "name": "tw-taupanga-chrome-extension-with-vue",
  "description": "A Vue.js web extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "web_accessible_resources": ["images/*.png", "images/*.jpg", "images/*.JPG"],
  "browser_action": {
    "default_title": "tw-taupanga-chrome-extension-with-vue",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "identity"
  ],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://apis.google.com http://tutoa.co.nz http://fontawesome.com/; object-src 'self'",
  "oauth2": {
    "client_id": "831532763147-99or786g95luevq7nhm8i32a95oame36.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
